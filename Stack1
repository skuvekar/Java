import java.util.Stack;
public class Stack1{
    public static void main(String[] args){
        Stack s = new Stack();
        s.push("Shruti");
        s.push(10);
        s.push("Dhriti");
        s.push(10);
        
        System.out.println(s.peek());
        System.out.println(s.pop());
        System.out.println(s);
        System.out.println(s.search("Dhriti"));
        System.out.println(s.empty());
        
    }
}
